{
    "extends": ["prettier", "standard"],
    "plugins": [
        "standard",
        "promise",
        "prettier"
    ],
    "rules": {
        "indent": ["error", 2],
        "no-invalid-regexp": 0,
        "no-useless-escape": 0,
        "comma-dangle": 0,
        "no-unused-vars": 0,
        "no-new": 0,
        "eqeqeq":0,
        "prettier/prettier": "warn",
        "semi": [0]
    }
}